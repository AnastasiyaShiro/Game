<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="283.0" prefWidth="453.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.task14.StartOrEnd">
   <children>
      <Label layoutX="125.0" layoutY="57.0" text="Добро пожаловать в онлайн казино!" />
      <Button layoutX="157.0" layoutY="101.0" mnemonicParsing="false" onAction="#startGame" text="Начать трату времени" />
      <Button layoutX="176.0" layoutY="181.0" mnemonicParsing="false" onAction="#endGame" text="Выйти и забыть" />
      <Label fx:id="namePlayer" layoutX="123.0" layoutY="142.0" text="Введите имя" visible="false" />
      <TextField fx:id="enterName" layoutX="204.0" layoutY="138.0" onKeyPressed="#switchToGame" prefHeight="25.0" prefWidth="118.0" visible="false" />
   </children>
</AnchorPane>
